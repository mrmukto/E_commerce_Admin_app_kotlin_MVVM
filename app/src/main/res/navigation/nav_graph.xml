<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.mrmukto.ecommerceadmin.DashboardFragment"
        android:label="DashBoard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_addProductFragment"
            app:destination="@id/addProductFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_orderFragment2"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_orderFragment3"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_categoryFragment"
            app:destination="@id/categoryFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_categoryFragment2"
            app:destination="@id/categoryFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_reportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_managerUserFragment"
            app:destination="@id/managerUserFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_viewProductFragment"
            app:destination="@id/viewProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mrmukto.ecommerceadmin.LoginFragment"
        android:label="Login"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/addProductFragment"
        android:name="com.mrmukto.ecommerceadmin.AddProductFragment"
        android:label="Add Product"
        tools:layout="@layout/fragment_add_product" />
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.mrmukto.ecommerceadmin.OrderFragment"
        android:label="Order"
        tools:layout="@layout/fragment_order" />
    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.mrmukto.ecommerceadmin.CategoryFragment"
        android:label="Category"
        tools:layout="@layout/fragment_category" />
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.mrmukto.ecommerceadmin.ReportFragment"
        android:label="Report"
        tools:layout="@layout/fragment_report" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.mrmukto.ecommerceadmin.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/managerUserFragment"
        android:name="com.mrmukto.ecommerceadmin.ManagerUserFragment"
        android:label="Manager User"
        tools:layout="@layout/fragment_manager_user" />
    <fragment
        android:id="@+id/viewProductFragment"
        android:name="com.mrmukto.ecommerceadmin.ViewProductFragment"
        android:label="View Product"
        tools:layout="@layout/fragment_view_product" >
        <action
            android:id="@+id/action_viewProductFragment_to_productDetailsFragment"
            app:destination="@id/productDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/productDetailsFragment"
        android:name="com.mrmukto.ecommerceadmin.ProductDetailsFragment"
        android:label="Product Details"
        tools:layout="@layout/fragment_product_details" />
</navigation>